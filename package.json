{
    "name": "babylon-zero",
    "version": "0.0.9",
    "description": "Different tryouts with babylon.JS",
    "author": "Christopher Stock <email@christopherstock.de>",
    "license": "MIT",
    "main": "dist/index.html",
    "homepage": "http://babylon.christopherstock.de",
    "repository": "https://github.com/christopherstock/babylon-zero.git",
    "bugs": "https://github.com/christopherstock/babylon-zero/issues",
    "scripts": {
        "build:dev": "webpack --mode=development --watch",
        "build:production": "webpack --mode=production --watch",
        "build:devserver": "webpack-dev-server --mode=development",
        "test:all": "mocha --require ts-node/register --require ignore-styles --require src/test/index.ts src/test/**/*.ts",
        "tslint:check": "tslint --project ./tsconfig.json --config ./tslint.json \"src/**/*.{ts,tsx}\"",
        "typedoc:generate": "typedoc --out doc\\ --listInvalidSymbolLinks --name \"babylon-zero, (c) 2019 Mayflower GmbH\" src/typescript"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "@types/node": "^9.6.48",
        "@types/webpack": "^4.4.32",
        "awesome-typescript-loader": "^5.2.1",
        "chai": "^4.2.0",
        "css-loader": "^0.28.11",
        "ignore-styles": "^5.0.1",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "mocha": "^5.2.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.20.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.16.0",
        "typedoc": "^0.11.1",
        "typescript": "^3.4.5",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "^3.4.1"
    },
    "dependencies": {
        "babylonjs": "^4.0.3",
        "babylonjs-gui": "^4.0.3",
        "cannon": "^0.6.2",
        "earcut": "^2.1.5",
        "moment": "^2.24.0"
    }
}
